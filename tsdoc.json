{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown",
    "./node_modules/tsdoc/template/plugins/TSDoc.js"
  ],
  "opts": {
    "template": "./node_modules/tsdoc/template",
    "recurse": "true"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "source": {
    "includePattern": "(\\.d)?\\.ts$"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "tsdoc": {
    "source": "./src",
    "destination": "./docs",
    "tutorials": "",
    "systemName": "Resizer",
    "footer": "",
    "copyright": "",
    "outputSourceFiles": true,
    "commentsOnly": true
  }
}
